{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: Script debugging",
      "cmakeDebugType": "script",
      "scriptPath": "${workspaceFolder}/<...>.cmake"
    },
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: Externally launched",
      "cmakeDebugType": "external",
      "pipeName": "<...>"
    },
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: Configure project",
      "cmakeDebugType": "configure",
      "clean": false,
      "configureAll": false
    },
    {
      "name": "(lldb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "enter program name, for example ${workspaceFolder}/a.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "(lldb) Attach",
      "type": "cppdbg",
      "request": "attach",
      "program": "enter program name, for example ${workspaceFolder}/a.out",
      "MIMode": "lldb"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "lldb",
      "request": "launch",
      "args": [],
      "cwd": "/Users/macbookairm2/Documents/GitHub/holbertonschool-low_level_programming/argc_argv",
      "program": "/Users/macbookairm2/Documents/GitHub/holbertonschool-low_level_programming/argc_argv/build/Debug/outDebug"
    }
  ]
}